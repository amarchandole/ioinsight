<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_weight="5"
        android:layout_height="0dp">
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/ioinsight"
        android:adjustViewBounds="true"/>
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="5">
    <TextView
        android:textColor="#000000"
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_gravity="center"
        android:textSize="30dp"
        android:layout_marginTop="20dp"
        android:text="CS 211 - IOinsight"
        android:textAppearance="?android:attr/textAppearanceSmall" />
    <TextView
        android:textColor="#000000"
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_gravity="center"
        android:textSize="25dp"
        android:layout_marginTop="60dp"
        android:text="Amar Chandole"
        android:textAppearance="?android:attr/textAppearanceSmall" />
        <TextView
            android:textColor="#000000"
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_gravity="center"
            android:textSize="25dp"
            android:layout_marginTop="100dp"
            android:text="Ameya Kabre"
            android:textAppearance="?android:attr/textAppearanceSmall" />
        <TextView
            android:textColor="#000000"
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_gravity="center"
            android:textSize="25dp"
            android:layout_marginTop="140dp"
            android:text="Saurabh Deo"
            android:textAppearance="?android:attr/textAppearanceSmall" />
        <TextView
            android:textColor="#000000"
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_gravity="center"
            android:textSize="25dp"
            android:layout_marginTop="180dp"
            android:text="Yuanzhi Gao"
            android:textAppearance="?android:attr/textAppearanceSmall" />
        <TextView
            android:textColor="#000000"
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_gravity="center"
            android:textSize="25dp"
            android:layout_marginTop="220dp"
            android:text="Mentored by: Prof Lu"
            android:textAppearance="?android:attr/textAppearanceSmall" />
    </RelativeLayout>
</LinearLayout>

    <!--android:background="@drawable/ioinsightlogo"-->